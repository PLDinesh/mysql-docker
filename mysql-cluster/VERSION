IMAGE_VERSION=1.2.15-cluster
LATEST="8.3"
LATEST_INNOVATION="8.3"

declare -A MYSQL_CLUSTER_VERSIONS
MYSQL_CLUSTER_VERSIONS["8.0"]=8.0.36
MYSQL_CLUSTER_VERSIONS["latest"]=8.3.0

declare -A MYSQL_SHELL_VERSIONS
MYSQL_SHELL_VERSIONS["8.0"]=8.0.36
MYSQL_SHELL_VERSIONS["latest"]=8.3.0

declare -A FULL_SERVER_VERSIONS
FULL_SERVER_VERSIONS["8.0"]="${MYSQL_CLUSTER_VERSIONS["8.0"]}-${IMAGE_VERSION}"
FULL_SERVER_VERSIONS["latest"]="${MYSQL_CLUSTER_VERSIONS["latest"]}-${IMAGE_VERSION}"
